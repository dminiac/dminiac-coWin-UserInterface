public interface UserService {

	public User saveUser(User user);

	public User getByState(String state);
	
	public User getByDistrict(String district);
	
	public User getByPincode(int pincode);
	
	public User updateUser(User user); //to update his user details
	
	public Details getUserById(int userId); //only can view his own details

}

